{
  "$schema": "./node_modules/@angular/pwa/node_modules/@schematics/angular/service-worker/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(png|jpg|jpeg|svg|webp)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-calls",
      "urls": [
        "/api/v1/**"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 100,
        "maxAge": "24h",
        "timeout": "10s"
      }
    }
  ],
  "project": "music-survey"
}
